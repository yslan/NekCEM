     $               , BFX    (LX1,LY1,LZ1,LELV)
     $               , BFY    (LX1,LY1,LZ1,LELV)
     $               , BFZ    (LX1,LY1,LZ1,LELV)
     $               , BX     (LX1,LY1,LZ1,LELV)
     $               , BY     (LX1,LY1,LZ1,LELV)
     $               , BZ     (LX1,LY1,LZ1,LELV)
     $               , PM     (LX1,LY1,LZ1,LELV)


