      COMMON /VPTSOL/  VX     (LX1,LY1,LZ1,LELV)
     $               , VY     (LX1,LY1,LZ1,LELV)
     $               , VZ     (LX1,LY1,LZ1,LELV)
     $               , PR     (LX2,LY2,LZ2,LELV)
