      common /newtonv/
     $     sn_k(lpts1,lcdim),
     $     gn_k(lpts1,lcdim),
     $     cni (lpts1,lcdim),
     $     cno (lpts1,lcdim),
     $     cn_k(lpts1,lcdim),
     $     fn  (lpts1,lcdim)
      real sn_k,gn_k,cni,cno,cn_k,fn

      common /newtons/ dtNT,dtNTinv,dtinv,epsinv,jaceps
      real dtNT,dtNTinv,dtinv,epsinv,jaceps

      common /newtoni/ iterNT
      integer iterNT
