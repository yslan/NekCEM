C
C     New zipper and surface plot data  (file GRPHCS)
C
      integer    LELT2,LLGRP,LG0,MSTRCT,LTOT
      PARAMETER (LELT2=2*LELT)
      COMMON /GCEN/  XCEN(LELT),YCEN(LELT),ZCEN(LELT),RCEN(LELT)
     $              ,XBMIN(LELT),YBMIN(LELT),ZBMIN(LELT) 
     $              ,XBMAX(LELT),YBMAX(LELT),ZBMAX(LELT) 
     $              ,XBMINT     ,YBMINT     ,ZBMINT      
     $              ,XBMAXT     ,YBMAXT     ,ZBMAXT      
      COMMON /QDPTS/ XYZQ(5,4,6,LELT)
      COMMON /QDPTI/ NLSTT,NLSTS,NLSTP
     $   ,LISTT(LELT2),LISTS(LELT2),LISTP(LELT2)
      COMMON /QDPTC/ CLISTS(LELT2)
      CHARACTER*20    CLISTS
      real  xcen,ycen,zcen,rcen
      real  xbmin,ybmin,zbmin
      real  xbmint,ybmint,zbmint
      real  xbmax,ybmax,zbmax  
      real  xbmaxt,ybmaxt,zbmaxt  
      real  xyzq
      integer   nlstt,nlsts,nlstp,listt,lists,listp
